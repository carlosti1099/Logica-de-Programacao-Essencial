        <body>
            <declare name="numero, limite, calcular" type="Integer" array="False" size=""/>
            <assign variable="numero" expression="1"/>
            <assign variable="limite" expression="10"/>
            <while expression="numero &lt;= limite">
                <output expression="&quot;Contagem Atual 9 x &quot; &amp; numero" newline="True"/>
                <assign variable="calcular" expression="9*numero"/>
